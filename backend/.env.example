# AudioNara Backend — Environment Variables
# Copy to .env (git-ignored) and fill in real values before running.

# ─── Server ──────────────────────────────────────────────────────────────────
APP_PORT=8080
APP_ENV=development          # development | production

# ─── PostgreSQL ──────────────────────────────────────────────────────────────
DB_HOST=localhost
DB_PORT=5432
DB_NAME=audionara
DB_USER=audionara_user
DB_PASSWORD=

# ─── Spotify API ─────────────────────────────────────────────────────────────
SPOTIFY_CLIENT_ID=
SPOTIFY_CLIENT_SECRET=

# ─── JWT ─────────────────────────────────────────────────────────────────────
JWT_SECRET=
JWT_EXPIRY_HOURS=24
